<?xml version="1.0"?>
<!--
**********************************************************************
Specific for AN-24B
**********************************************************************
-->
<PropertyList>

  <adf>
    <name>adf</name>
    <number>0</number>
  </adf>

  <adf>
    <name>adf</name>
    <number>1</number>
  </adf>

  <adf>
    <name>adf</name>
    <number>2</number>
  </adf>

  <adf>
    <name>adf</name>
    <number>3</number>
  </adf>

<!-- Left KUS730 -->
  <airspeed-indicator>
    <name>airspeed-indicator</name>
    <number>0</number>
    <total-pressure>/an24/Pressure/A1orA2pressure-inhg</total-pressure>
    <static-pressure>/an24/Pressure/S1orE1pressure-inhg</static-pressure>
  </airspeed-indicator>

<!-- Right KUS730 -->
  <airspeed-indicator>
    <name>airspeed-indicator</name>
    <number>1</number>
    <total-pressure>/an24/Pressure/PitotA3_out</total-pressure>
    <static-pressure>/an24/Pressure/S2orE2pressure-inhg</static-pressure>
  </airspeed-indicator>

<!-- UVO-15M (left) -->
  <altimeter>
    <name>altimeter</name>
    <number>0</number>
    <encode-mode-c type="bool">false</encode-mode-c>
    <encode-mode-s type="bool">false</encode-mode-s>
    <static-pressure>/an24/Pressure/S1orE1pressure-inhg</static-pressure>
    <quantum>0</quantum>
    <tau>0</tau>
  </altimeter>

<!-- UVID-15 (right ) -->
  <altimeter>
    <name>altimeter</name>
    <number>1</number>
    <encode-mode-c type="bool">true</encode-mode-c>
    <encode-mode-s type="bool">false</encode-mode-s>
    <static-pressure>/an24/Pressure/S2orE2pressure-inhg</static-pressure>
    <quantum>0</quantum>
    <tau>0</tau>
  </altimeter>

  <attitude-indicator>
    <name>agd-l</name>
    <number>0</number>
    <suction>an24/AGD/agd-l</suction>
  </attitude-indicator>

  <attitude-indicator>
    <name>agd-r</name>
    <number>1</number>
    <suction>an24/AGD/agd-r</suction>
  </attitude-indicator>

  <clock>
    <name>clock</name>
    <number>0</number>
  </clock>

  <clock>
    <name>clock</name>
    <number>1</number>
  </clock>

  <clock>
    <name>clock</name>
    <number>2</number>
  </clock>

<!--R-802 No.1-->
  <comm-radio>
    <name>comm</name>
    <number>0</number>
    <eight-point-three>0</eight-point-three>
    <full-duplex>0</full-duplex>
  </comm-radio>

<!--R-802 No.2-->
  <comm-radio>
    <name>comm</name>
    <number>1</number>
    <eight-point-three>0</eight-point-three>
    <full-duplex>0</full-duplex>
  </comm-radio>

  <dme>
    <name>dme</name>
    <number>0</number>
  </dme>

  <marker-beacon>
    <name>marker-beacon</name>
    <number>0</number>
  </marker-beacon>
  
  <magnetic-compass>
    <name>magnetic-compass</name>
    <number>0</number>
  </magnetic-compass>

  <nav-radio>
    <name>nav</name>
    <number>0</number>
  </nav-radio>

  <nav-radio>
    <name>nav</name>
    <number>1</number>
  </nav-radio>

  <radar-altimeter>
    <name>radar-altimeter</name>
    <number>0</number>
    <update-interval-sec>0.1</update-interval-sec>
  </radar-altimeter>

  <slip-skid-ball>
    <name>slip-skid-ball</name>
    <number>0</number>
  </slip-skid-ball>

<!--Replaced by own code-->
  <!--transponder>
    <name>transponder</name>
    <number>0</number>
    <mode type="int">1</mode>
    <encoder-path type="string">instrumentation/altimeter[1]</encoder-path>
    <kt70-compatibility type="bool">0</kt70-compatibility>
  </transponder-->

  <turn-indicator>
    <name>turn-indicator</name>
    <number>0</number>
  </turn-indicator>

  <vertical-speed-indicator>
    <name>vertical-speed-indicator</name>
    <number>0</number>
    <static-pressure>/an24/Pressure/S1orE1pressure-inhg</static-pressure>
  </vertical-speed-indicator>

  <vertical-speed-indicator>
    <name>vertical-speed-indicator</name>
    <number>1</number>
    <static-pressure>/an24/Pressure/S2orE2pressure-inhg</static-pressure>
  </vertical-speed-indicator>

</PropertyList>
