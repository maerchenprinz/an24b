<?xml version="1.0"?>

<PropertyList>
 <path>cockpit.ac</path>

<!--Cockpit animations-->

<!--Yoke-->
<animation>
  <type>rotate</type>
  <object-name>yokeL</object-name>
  <property>/controls/flight/aileron</property>
  <factor>90.0</factor>
  <offset-deg>0.0</offset-deg>
  <center>
    <x-m>0.0</x-m>
    <y-m>-0.4788</y-m>
    <z-m>0.8728</z-m>
  </center>
  <axis>
    <x>-1</x>
    <y>0</y>
    <z>0</z>    
  </axis>
</animation>

<animation>
  <type>translate</type>
  <object-name>yokeL</object-name>
  <property>/controls/flight/elevator</property>
  <factor>0.08</factor>
  <offset-deg>0.0</offset-deg>
  <axis>
    <x>-1</x>
    <y>0</y>
    <z>0</z>    
  </axis>
</animation>

<animation>
  <type>rotate</type>
  <object-name>yokeR</object-name>
  <property>/controls/flight/aileron</property>
  <factor>90.0</factor>
  <offset-deg>0.0</offset-deg>
  <center>
    <x-m>0.0</x-m>
    <y-m>0.4788</y-m>
    <z-m>0.8728</z-m>
  </center>
  <axis>
    <x>-1</x>
    <y>0</y>
    <z>0</z>    
  </axis>
</animation>

<animation>
  <type>translate</type>
  <object-name>yokeR</object-name>
  <property>/controls/flight/elevator</property>
  <factor>0.08</factor>
  <offset-deg>0.0</offset-deg>
  <axis>
    <x>-1</x>
    <y>0</y>
    <z>0</z>    
  </axis>
</animation>

<!--Front seats-->

<animation>
  <type>pick</type>
  <object-name>armrest</object-name>
  <visible>true</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
        if( getprop("an24/animations/armrest" ) == 1.0 ){
        interpolate("an24/animations/armrest", 0.0, 1.0 );
        }
        else {
        interpolate("an24/animations/armrest", 1.0, 1.0 );
        }
      </script>
    </binding>
  </action>
</animation>

<animation>
  <type>rotate</type>
  <object-name>armrest</object-name>
  <property>an24/animations/armrest</property>
  <factor>80.0</factor>
  <center>
    <x-m>2.9062</x-m>
    <y-m>0.5939</y-m>
    <z-m>0.6851</z-m>    
  </center>
  <axis>
    <x>0</x>
    <y>-1</y>
    <z>0</z>    
  </axis>
</animation>


<!--Gustlock-->
<animation>
 <type>select</type>
 <condition>
   <equals>
    <property>an24/animations/gustlock</property>
    <value>1</value>
   </equals>
  </condition>
 <object-name>gustlock</object-name>
</animation>

<animation>
  <type>pick</type>
  <object-name>gustlock</object-name>
  <visible>true</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>property-toggle</command>
      <property>an24/animations/gustlock</property>
       <value>0</value>
       <value>1</value>
    </binding>
  </action>
</animation>

<!--Trimwheel-->
<animation>
  <type>rotate</type>
  <object-name>trimwheel</object-name>
  <property>/controls/flight/elevator-trim</property>
  <factor>360.0</factor>
  <offset-deg>0.0</offset-deg>
  <center>
    <x-m>2.26298</x-m>
    <y-m>0.0</y-m>
    <z-m>0.59665</z-m>
  </center>
  <axis>
    <x>0</x>
    <y>-1</y>
    <z>0</z>    
  </axis>
</animation>

<!-- Navigator's center seat-->
<animation>
  <type>pick</type>
  <object-name>centerseatbottom</object-name>
  <visible>true</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
        if( getprop("an24/animations/centerseat" ) == 1.0 ){
        interpolate("an24/animations/centerseat", 0.0, 3.0 );
        }
        else {
        interpolate("an24/animations/centerseat", 1.0, 3.0 );
        }
      </script>
    </binding>
  </action>
</animation>

<animation>
  <type>rotate</type>
  <object-name>centerseatbottom</object-name>
  <property>an24/animations/centerseat</property>
  <factor>183.0</factor>
  <center>
    <x-m>3.12607</x-m>
    <y-m>0.0</y-m>
    <z-m>0.11897</z-m>    
  </center>
  <axis>
    <x>0</x>
    <y>-1</y>
    <z>0</z>    
  </axis>
</animation>

<animation>
  <type>rotate</type>
  <object-name>centerseatrest</object-name>
  <property>an24/animations/centerseat</property>
  <factor>183.0</factor>
  <center>
    <x-m>3.12607</x-m>
    <y-m>0.0</y-m>
    <z-m>0.11897</z-m>    
  </center>
  <axis>
    <x>0</x>
    <y>-1</y>
    <z>0</z>    
  </axis>
</animation>

<animation>
  <type>rotate</type>
  <object-name>centerseatrest</object-name>
  <property>an24/animations/centerseat</property>
  <factor>100.0</factor>
  <center>
    <x-m>2.957</x-m>
    <y-m>0.0</y-m>
    <z-m>-0.2</z-m>    
  </center>
  <axis>
    <x>0</x>
    <y>1</y>
    <z>0</z>    
  </axis>
</animation>


<!--Windows-->
<animation>
  <type>pick</type>
  <object-name>windowL</object-name>
  <visible>true</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>property-toggle</command>
      <property>an24/animations/windowL</property>
       <value>0</value>
       <value>1</value>
    </binding>
  </action>
</animation>

<animation>
  <type>translate</type>
  <object-name>windowL</object-name>
  <property>an24/animations/windowL</property>
  <factor>0.8</factor>
  <axis>
    <x>1</x>
    <y>0</y>
    <z>0</z>    
  </axis>
</animation>



