  <factor>183.0</factor>
  <center>
    <x-m>0.3827</x-m>
    <y-m>0.0</y-m>
    <z-m>-0.2587</z-m>
  </center>
  <axis>
    <x>0</x>
    <y>-1</y>
    <z>0</z>    
  </axis>
</animation>

<animation>
  <type>rotate</type>
  <object-name>centerseatrest</object-name>
  <property>an24/animations/centerseat</property>
  <factor>100.0</factor>
  <center>
    <x-m>0.204</x-m>
    <y-m>0.0</y-m>
    <z-m>-0.612</z-m>    
  </center>
  <axis>
    <x>0</x>
    <y>1</y>
    <z>0</z>    
  </axis>
</animation>


<!--Trimwheel-->
<animation>
  <type>rotate</type>
