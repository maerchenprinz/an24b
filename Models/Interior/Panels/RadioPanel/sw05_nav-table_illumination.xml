<?xml version="1.0"?>

<!-- Navigator table light -->
<PropertyList>
 <path>../../shared/switches/azs.ac</path>

 <animation>
  <type>pick</type>
  <object-name>clickspot_up</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
	interpolate("an24/Radio_Panel/sw_navtablelight", 1.0, 0.1 );
	sound.click("azr", 3.9, 1.0, 0.8);
      </script>
    </binding>
  </action>
   <hovered>
     <binding>
      <command>set-tooltip</command>
      <tooltip-id>navtablelighton</tooltip-id>
      <label>Navigator's Table
     light ON</label>
     </binding>
   </hovered>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>clickspot_down</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
	interpolate("an24/Radio_Panel/sw_navtablelight", 0.0, 0.1 );
	sound.click("azr", 3.9, 1.0, 0.8);
      </script>
    </binding>
  </action>
   <hovered>
     <binding>
      <command>set-tooltip</command>
      <tooltip-id>navtablelightoff</tooltip-id>
      <label>Navigator's Table
     light Off</label>
     </binding>
   </hovered>
 </animation>

 <animation>
   <type>rotate</type>
   <object-name>lever</object-name>
   <property>an24/Radio_Panel/sw_navtablelight</property>
  <interpolation>
    <entry><ind>1.0</ind><dep>20.0</dep></entry>
    <entry><ind>0.0</ind><dep>-20.0</dep></entry>
  </interpolation>
  <axis>
    <x>0.0</x>
    <y>-1.0</y>
    <z>0.0</z>
  </axis>
 </animation>

</PropertyList>
