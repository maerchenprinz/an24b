<!-- Hatch above RK Crew Cabin -->
 <animation>
   <type>knob</type>
   <object-name>table_hatch</object-name>
   <property>an24/RKCrew/table_hatch</property>
   <factor>70.0</factor>
   <axis>
    <x>-1</x>
    <y>0</y>
    <z>0</z>
   </axis>
   <center>
    <x-m>0.0</x-m>
    <y-m>1.147</y-m>
    <z-m>0.323</z-m>
   </center>
    <action>
    <binding>
      <command>nasal</command>
      <script>
	if( getprop("sim/current-view/view-number" ) == 10 ){
		if( getprop("an24/RKCrew/table_hatch" ) == 1.0 ){
		interpolate("an24/RKCrew/table_hatch", 1.2, 0.2, 0.0, 0.5 );
		}
 		else {
		interpolate("an24/RKCrew/table_hatch", 1.2, 0.5, 1.0, 0.2 );
		}
	}
      </script>
    </binding>
    </action>
 </animation>

