 <animation>
   <type>material</type>
   <object-name>adf_housing</object-name>
   <object-name>adf_compass</object-name>
   <object-name>adf_needle_1</object-name>
   <object-name>adf_needle_2</object-name>
   <emission>
     <red>1.0</red>
     <green>0.4</green>
     <blue>0.3</blue>
     <factor-prop>controls/lighting/nav_panel_instr_red</factor-prop>
   </emission>
 </animation>

<!-- Hot air foot pedal -->
 <animation>
   <type>knob</type>
   <object-name>heatpedalENG</object-name>
   <property>an24/Air-Cond/radioop_footpedal</property>
   <factor>30.0</factor>
   <offset-deg>5</offset-deg>
   <axis>
    <x>0</x>
    <y>-1</y>
    <z>0</z>
   </axis>
   <center>
    <x-m>3.935</x-m>
    <y-m>0.0</y-m>
    <z-m>-0.341</z-m>
   </center>
    <action>
     <binding>
      <command>property-adjust</command>
      <property>an24/Air-Cond/radioop_footpedal</property>
      <factor>-0.2</factor>
      <min>0</min>
      <max>1</max>                
      <wrap>false</wrap>
     </binding>
    </action>
 </animation>

<!-- Air Conditioning Handles -->
 <animation>
   <type>knob</type>
   <object-name>aircond_handleL</object-name>
   <property>an24/Air-Cond/handle_left</property>
   <factor>-90</factor>
  <center>
   <x-m>3.1586</x-m>
   <y-m>0.0</y-m>
   <z-m>0.853</z-m>
  </center>
  <axis>
    <x>0.0</x>
    <y>1.0</y>
    <z>0.0</z>
  </axis>
    <action>
     <binding>
      <command>property-adjust</command>
      <property>an24/Air-Cond/handle_left</property>
      <factor>-0.05</factor>
      <min>0</min>
      <max>1</max>
      <wrap>false</wrap>
     </binding>
    </action>
   <hovered>
    <binding>
     <command>set-tooltip</command>
     <label>Window Air Left: %3d%%</label>
     <tooltip-id>air-left</tooltip-id>
     <mapping>percent</mapping>
     <property>an24/Air-Cond/handle_left</property>
    </binding>
   </hovered>
 </animation>

 <animation>
   <type>knob</type>
   <object-name>aircond_handleR</object-name>
   <property>an24/Air-Cond/handle_right</property>
   <factor>-90</factor>
  <center>
   <x-m>3.24</x-m>
   <y-m>0.0</y-m>
   <z-m>0.756</z-m>
  </center>
  <axis>
    <x>0.0</x>
    <y>1.0</y>
    <z>0.0</z>
  </axis>
    <action>
     <binding>
      <command>property-adjust</command>
      <property>an24/Air-Cond/handle_right</property>
      <factor>-0.05</factor>
      <min>0</min>
      <max>1</max>
      <wrap>false</wrap>
     </binding>
    </action>
   <hovered>
    <binding>
     <command>set-tooltip</command>
     <label>Window Air Right: %3d%%</label>
     <tooltip-id>air-right</tooltip-id>
     <mapping>percent</mapping>
     <property>an24/Air-Cond/handle_right</property>
    </binding>
   </hovered>
 </animation>

<!-- Crew door -->
 <animation>
  <type>pick</type>
  <object-name>crewdoor</object-name>
  <visible>true</visible>
  <action>
   <button>0</button>
   <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
	if( getprop("an24/DoorsHatches/crewdoor" ) == 0.0 ) {
	interpolate("an24/DoorsHatches/crewdoor", 1.0, 1.0 );
	}
	else {
	interpolate("an24/DoorsHatches/crewdoor", 0.0, 1.0 );
	}
      </script>
    </binding>
  </action>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>crewdoor</object-name>
  <property>an24/DoorsHatches/crewdoor</property>
  <interpolation>
   <entry><ind>0.0</ind><dep>0.0</dep></entry>
   <!--entry><ind>0.5</ind><dep>0.0</dep></entry-->
   <entry><ind>1.0</ind><dep>92.0</dep></entry>
  </interpolation>
  <center>
    <x-m>4.015</x-m>
    <y-m>-0.276</y-m>
    <z-m>0.0</z-m>    
  </center>
  <axis>
    <x>0</x>
    <y>0</y>
    <z>-1</z>    
  </axis>
 </animation>

