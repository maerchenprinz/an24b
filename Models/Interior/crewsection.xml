<?xml version="1.0"?>

<PropertyList>
 <path>crewsection.ac</path>

<animation>
  <type>pick</type>
  <object-name>cockpitdoor</object-name>
  <visible>true</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
        if( getprop("an24/animations/cockpitdoor" ) == 1.0 ){
        interpolate("an24/animations/cockpitdoor", 0.0, 1.0 );
        }
        else {
        interpolate("an24/animations/cockpitdoor", 1.0, 1.0 );
        }
      </script>
    </binding>
  </action>
</animation>

<animation>
  <type>rotate</type>
  <object-name>cockpitdoor</object-name>
  <property>an24/animations/cockpitdoor</property>
  <factor>80.0</factor>
  <center>
    <x-m>3.9792</x-m>
    <y-m>-0.2841</y-m>
    <z-m>1.2262</z-m>    
  </center>
  <axis>
    <x>0</x>
    <y>0</y>
    <z>-1</z>    
  </axis>
</animation>

