<!-- Frame left-right switch -->
 <animation>
  <type>pick</type>
  <object-name>clickspot_left</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>true</repeatable>

<!-- Mode "Compass I/II" -->
     <binding>
      <command>property-adjust</command>
      <property>an24/ARK-11/frame-offset-1</property>
       <condition>
        <equals>
         <property>an24/ARK-11/power-1_115V</property>
         <value>1.0</value>
        </equals>
        <equals>
         <property>an24/ARK-11/panel_nav_oh</property>
         <value>0.0</value>
        </equals>
        <or>
        <equals>
         <property>an24/ARK-11/mode-nav</property>
         <value>1</value>
        </equals>
        <equals>
         <property>an24/ARK-11/mode-nav</property>
         <value>4</value>
        </equals>
        </or>
       </condition>
      <step>4</step>
      <min>-180</min>
      <max>180</max>                
      <wrap>true</wrap>
     </binding>
<!-- Mode "Antenna" -->
     <binding>
      <command>property-adjust</command>
      <property>an24/ARK-11/frame-offset-1</property>
       <condition>
        <and>
        <equals>
         <property>an24/ARK-11/power-1_115V</property>
         <value>1.0</value>
        </equals>
        <equals>
         <property>an24/ARK-11/mode-nav</property>
         <value>2</value>
        </equals>
        <equals>
         <property>an24/ARK-11/panel_nav_oh</property>
         <value>0.0</value>
        </equals>
        </and>
       </condition>
      <step>1</step>
      <min>-180</min>
      <max>180</max>
      <wrap>true</wrap>
     </binding>
<!-- Mode "Frame" -->
     <binding>
      <command>property-adjust</command>
      <property>instrumentation/adf[0]/indicated-bearing-deg</property>
       <condition>
        <and>
        <equals>
         <property>an24/ARK-11/power-1_27V</property>
         <value>1.0</value>
        </equals>
        <equals>
         <property>an24/ARK-11/mode-nav</property>
         <value>3</value>
        </equals>
        <equals>
         <property>an24/ARK-11/panel_nav_oh</property>
         <value>0.0</value>
        </equals>
        </and>
       </condition>
      <step>-1</step>
      <!--min>-180</min>
      <max>180</max>
      <wrap>true</wrap-->
     </binding>

    <binding>
      <command>nasal</command>
      <script>
        interpolate("an24/ARK-11/frame_l-r-1", -1.0, 0.1 );
      </script>
    </binding>
    <mod-up>
      <binding>
      <command>nasal</command>
      <script>
        interpolate("an24/ARK-11/frame_l-r-1", 0.0, 0.1 );
      </script>
    </binding>
    </mod-up>
  </action>
</animation>

