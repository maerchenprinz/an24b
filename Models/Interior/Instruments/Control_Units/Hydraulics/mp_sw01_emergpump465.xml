<?xml version="1.0"?>

<!-- Pump 465 of emergency hydraulic pump, bottom of middle panel, switch 1 -->
<PropertyList>
 <path>../../../shared/switches/vg-15k.ac</path>

 <animation>
  <type>pick</type>
  <object-name>clickspot_up</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
        interpolate("an24/Hydraulics/sw_emergpump", 1.0, 0.1 );
	sound.click("azr", 2.1, -0.36, 0.65);
      </script>
    </binding>
  </action>
  <hovered>
    <binding>
     <command>set-tooltip</command>
     <tooltip-id>hydemergpumpon</tooltip-id>
      <label>Hydr. Emergency
     Pump ON</label>
    </binding>
  </hovered>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>clickspot_down</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
        interpolate("an24/Hydraulics/sw_emergpump", 0.0, 0.1 );
	sound.click("azr", 2.1, -0.36, 0.65);
      </script>
    </binding>
  </action>
  <hovered>
    <binding>
     <command>set-tooltip</command>
     <tooltip-id>hydemergpumpoff</tooltip-id>
      <label>Hydr. Emergency
     Pump OFF</label>
    </binding>
  </hovered>
 </animation>

 <animation>
   <type>rotate</type>
   <object-name>lever</object-name>
   <property>an24/Hydraulics/sw_emergpump</property>
  <interpolation>
    <entry><ind>0.0</ind><dep>-25.0</dep></entry>
    <entry><ind>1.0</ind><dep>25.0</dep></entry>
  </interpolation>
  <axis>
    <x>0.0</x>
    <y>-1.0</y>
    <z>0.0</z>
  </axis>
 </animation>

</PropertyList>
