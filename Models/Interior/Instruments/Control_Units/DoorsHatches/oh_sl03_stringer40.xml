<?xml version="1.0"?>

<!-- Pressure door at stringer 40 open -->
<PropertyList>
 <path>../../../shared/lamps/warninglamp02.ac</path>

 <animation>
  <type>material</type>
  <object-name>holder</object-name>
  <object-name>lampon</object-name>
  <texture>../../../shared/lamps/warninglamp02green.png</texture>
 </animation>

 <animation>
  <type>select</type>
  <condition>
   <and>
    <greater-than>
     <property>an24/Electrics/AZSmain_V</property>
     <value>22.0</value>
    </greater-than>
    <equals>
     <property>an24/AZS/sw0315</property>
     <value>1.0</value>
    </equals>
    <greater-than>
     <property>an24/doorshatches/test</property>
     <value>0.8</value>
    </greater-than>
   </and>
  </condition>
  <object-name>lampon</object-name>
 </animation>

</PropertyList>
