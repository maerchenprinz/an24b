<?xml version="1.0"?>

<!-- Gear switch to prevent  -->

<PropertyList>
  <path>../../../shared/switches/vg-15.ac</path>

  <animation>
    <type>pick</type>
    <object-name>clickspot_up</object-name>
    <visible>false</visible>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
       <command>nasal</command>
       <script>
	interpolate("an24/gear/sw_gear_lock-on-ground", 1.0, 0.1 );
       </script>
      </binding>
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>clickspot_down</object-name>
    <visible>false</visible>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
       <command>nasal</command>
       <script>
	interpolate("an24/gear/sw_gear_lock-on-ground", 0.0, 0.1 );
       </script>
      </binding>
    </action>
  </animation>

 <animation>
   <type>rotate</type>
   <object-name>lever</object-name>
   <property>an24/gear/sw_gear_lock-on-ground</property>
  <interpolation>
    <entry><ind>1.0</ind><dep>25.0</dep></entry>
    <entry><ind>0.0</ind><dep>-25.0</dep></entry>
  </interpolation>
  <axis>
    <x>0.0</x>
    <y>-1.0</y>
    <z>0.0</z>
  </axis>
 </animation>

</PropertyList>
