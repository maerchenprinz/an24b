<?xml version="1.0"?>

<!-- Tank-group 1 pump ETsN-14 (main) left set to auto(1)/off(0)/manual(-1)-->

<PropertyList>
 <path>../../shared/switches/switch03.ac</path>
 
 <animation>
  <type>pick</type>
  <object-name>clickspot_up</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
        setprop("an24/FuelControl/group1leftpump", 1.0 );
        interpolate("an24/animations/FuelControl/group1leftpump", 1.0, 0.1 );
      </script>
    </binding>
  </action>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>clickspot_middle</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
        setprop("an24/FuelControl/group1leftpump", 0.0 );
        interpolate("an24/animations/FuelControl/group1leftpump", 0.0, 0.1 );
      </script>
    </binding>
  </action>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>clickspot_down</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
        setprop("an24/FuelControl/group1leftpump", -1.0 );
        interpolate("an24/animations/FuelControl/group1leftpump", -1.0, 0.1 );
      </script>
    </binding>
  </action>
</animation>

 <animation>
   <type>rotate</type>
   <object-name>lever</object-name>
   <property>an24/animations/FuelControl/group1leftpump</property>
  <interpolation>
    <entry><ind>-1.0</ind>	<dep>-35.0</dep></entry>
    <entry><ind>1.0</ind>	<dep>35.0</dep></entry>
  </interpolation>
  <axis>
    <x>0.0</x>
    <y>-1.0</y>
    <z>0.0</z>
  </axis>
 </animation>

</PropertyList>
