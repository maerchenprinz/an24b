  <center>
    <x-m>-0.0173</x-m>
    <y-m>0</y-m>
    <z-m>-0.0161</z-m>
  </center>
  <axis>
    <x>0.0</x>
    <y>-1.0</y>
    <z>0.0</z>
  </axis>
</animation>

