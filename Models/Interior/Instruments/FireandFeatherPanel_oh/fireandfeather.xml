<!-- ************ Fire section *********** -->

<model>
 <path>btn01.xml</path>
  <offsets>
    <x-m>-0.0136</x-m>
    <y-m>0.1317</y-m>
    <z-m>-0.0483</z-m>
    <pitch-deg>90</pitch-deg>
  </offsets>
</model>

<animation>
  <type>pick</type>
  <object-name>cover01</object-name>
  <visible>true</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
        if( getprop("an24/animations/FireFeather/cover01" ) == 1.0 ){
        interpolate("an24/animations/FireFeather/cover01", 0.0, 0.2 );
        }
        else {
        interpolate("an24/animations/FireFeather/cover01", 1.0, 0.2 );
        }
      </script>
    </binding>
  </action>
</animation>

 <animation>
   <type>rotate</type>
   <object-name>cover01</object-name>
   <property>an24/animations/FireFeather/cover01</property>
   <factor>80</factor>
  <center>
    <x-m>0.0047</x-m>
    <y-m>0.0</y-m>
    <z-m>-0.0694</z-m>
  </center>
  <axis>
    <x>0</x>
    <y>1</y>
    <z>0</z>
  </axis>
 </animation>

