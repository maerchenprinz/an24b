     <y-m>0.15</y-m>
     <z-m>0.875</z-m>
     <scale>8</scale>
    </marker>
   <wait>3</wait>
  </step>

  <step>
    <message>
      	FUEL PUMPS check... MAIN GROUP PUMPS...
	up is AUTO
    </message>
